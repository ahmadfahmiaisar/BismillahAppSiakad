<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="inn.mroyek.bismillahsiakad">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MySiakad"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_uny"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_config"
        android:roundIcon="@mipmap/ic_launcher_uny_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="n">
        <activity android:name=".presentation.ui.khs.KhsActivity"></activity>
        <activity android:name=".presentation.ui.inputnilai.InputNilaiActivity" />
        <activity android:name=".presentation.ui.dhs.DhsActivity" />
        <activity android:name=".presentation.ui.krs.parent.KrsActivity" />
        <activity android:name=".presentation.ui.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.ui.login.LoginActivity" />
    </application>

</manifest>